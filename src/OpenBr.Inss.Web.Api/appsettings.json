{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "IncludeScopes": "false"
    }
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "Open Brasil INSS",
        "Uri": "http://localhost:5000/selfcheck"
      }
    ],
    "EvaluationTimeInSeconds": 10,
    "MinimumSecondsBetweenFailureNotifications": 60,
    "HealthCheckDatabaseConnectionString": "Data Source=wwwroot/data/hc.db"
  },
  "AllowedHosts": "*"
}
